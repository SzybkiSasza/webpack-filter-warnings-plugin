{
  "name": "webpack-filter-warnings-plugin",
  "version": "1.2.1",
  "description": "Allows you to hide certain warnings from webpack compilations",
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:typescript && npm run build:babel",
    "build:babel": "babel ts-out --out-dir . --copy-files ",
    "build:cjs": "tsc -p './build-tools/tsconfig-cjs.json'",
    "build:dev": "npm run build -- -- --source-maps inline",
    "build:es": "tsc -p './build-tools/tsconfig-es.json'",
    "build:typescript": "npm run build:cjs && npm run build:es && npm run rename-indexes",
    "clean": "del-cli es cjs ts-out",
    "lint": "tslint 'build-tools/**/*.ts' 'src/**/*.ts' 'test/**/*.ts' --project tsconfig.json",
    "lint-staged": "lint-staged",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "npm run build",
    "rename-indexes": "ts-node ./build-tools/rename-indexes.ts",
    "release": "standard-version",
    "security": "npm audit --audit-level high",
    "test": "jest --config jest.config.js",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "travis:coverage": "npm run test:coverage",
    "travis:lint": "npm run lint && npm run security",
    "travis:test": "npm test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattlewis92/webpack-filter-warnings-plugin.git"
  },
  "keywords": [
    "webpack plugin"
  ],
  "author": "Matt Lewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlewis92/webpack-filter-warnings-plugin/issues"
  },
  "homepage": "https://github.com/mattlewis92/webpack-filter-warnings-plugin#readme",
  "files": [
    "cjs",
    "es"
  ],
  "engines": {
    "node": ">= 4.3 < 5.0.0 || >= 5.10"
  },
  "peerDependencies": {
    "webpack": "^2.0.0 || ^3.0.0 || ^4.0.0"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@types/glob": "^7.1.1",
    "@types/jest": "^23.3.14",
    "@types/memory-fs": "^0.3.2",
    "del-cli": "^1.1.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "memory-fs": "^0.4.1",
    "pre-commit": "^1.2.2",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3"
  },
  "dependencies": {
    "@types/webpack": "^4.4.24"
  }
}
